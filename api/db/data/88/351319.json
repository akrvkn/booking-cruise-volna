u0434\u043e\u043c\u0430 \u0440\u0435\u043c\u0435\u0441\u0435\u043b, \u0423\u0441\u043f\u0435\u043d\u0441\u043a\u043e\u0433\u043e \u0441\u043e\u0431\u043e\u0440\u0430. (\u0433\u0440\u0443\u043f\u043f\u0430 \u043e\u0442 25 \u0447\u0435\u043b.) \u041f\u0440\u043e\u0434\u043e\u043b\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u0438 2-2,5&nbsp; \u0447\u0430\u0441\u0430.\r\n\r\n\t\t\r\n\t\t\t2) \u041f\u0435\u0448\u0435\u0445\u043e\u0434\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f &laquo;\u0414\u043b\u044f \u0434\u0435\u0442\u0435\u0439 \u0438 \u0438\u0445 \u043f\u043e\u0441\u043b\u0443\u0448\u043d\u044b\u0445 \u0440\u043e\u0434\u0438\u0442\u0435\u043b\u0435\u0439&raquo;. \u041f\u043e\u0441\u0435\u0449\u0435\u043d\u0438\u0435 \u043a\u043e\u043c\u043f\u043b\u0435\u043a\u0441\u0430 &laquo;\u0414\u0432\u043e\u0440\u0435\u0446 \u041c\u044b\u0448\u0438&raquo;. (\u0433\u0440\u0443\u043f\u043f\u0430 \u043e\u0442 25 \u0447\u0435\u043b.) \u041f\u0440\u043e\u0434\u043e\u043b\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u0438 1,5-2,0 \u0447\u0430\u0441\u0430.\r\n\r\n\t\t\r\n\t\t\t3) \u041f\u0435\u0448\u0435\u0445\u043e\u0434\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f &laquo;\u041f\u0440\u043e\u0432\u0438\u043d\u0446\u0438\u0430\u043b\u044c\u043d\u044b\u0439 \u043a\u0430\u043b\u0435\u0439\u0434\u043e\u0441\u043a\u043e\u043f&raquo;. \u041e\u0431\u0437\u043e\u0440\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f \u043f\u043e \u0433\u043e\u0440\u043e\u0434\u0443 \u0441 \u043e\u0441\u043c\u043e\u0442\u0440\u043e\u043c \u0434\u0435\u0440\u0435\u0432\u044f\u043d\u043d\u044b\u0445 \u0434\u043e\u043c\u043e\u0432 (\u0434\u043e\u043c \u043a\u043e\u0440\u0430\u0431\u0435\u043b\u0430 \u0422\u043e\u043c\u0441\u043e\u043d\u0430, \u0434\u043e\u043c-\u0432\u043e\u043a\u0437\u0430\u043b \u0433\u0440\u0430\u0444\u0430 \u0428\u0435\u0440\u0435\u043c\u0435\u0442\u044c\u0435\u0432\u0430), \u043f\u043e\u0441\u0435\u0449\u0435\u043d\u0438\u0435 \u0441\u0442\u0430\u0440\u0438\u043d\u043d\u043e\u0439 \u043c\u0435\u043b\u044c\u043d\u0438\u0446\u044b \u0441 \u044d\u043a\u0441\u043f\u043e\u0437\u0438\u0446\u0438\u0435\u0439 &laquo;\u0410\u043c\u0431\u0430\u0440\u043d\u044b\u0435 \u043c\u044b\u0448\u0438&raquo;, \u041c\u0435\u043c\u043e\u0440\u0438\u0430\u043b\u0430 \u041f\u043e\u0431\u0435\u0434\u044b, \u0423\u0441\u043f\u0435\u043d\u0441\u043a\u043e\u0433\u043e \u0441\u043e\u0431\u043e\u0440\u0430.&nbsp; (\u0433\u0440\u0443\u043f\u043f\u0430 \u043e\u0442 25 \u0447\u0435\u043b.) \u041f\u0440\u043e\u0434\u043e\u043b\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u0438 2,5 \u0447\u0430\u0441\u0430.\r\n\t\r\n\r\n\u0437\u0430 \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u0443\u044e \u043f\u043b\u0430\u0442\u0443\r\n\t\u041f\u0435\u0448\u0435\u0445\u043e\u0434\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f &laquo;\u041c\u044b\u0448\u043a\u0438\u043d\u0441\u043a\u0438\u0439 \u043d\u0430\u0440\u043e\u0434\u043d\u044b\u0439 \u043c\u0443\u0437\u0435\u0439&raquo; \u0441 \u043f\u043e\u0441\u0435\u0449\u0435\u043d\u0438\u0435\u043c \u043c\u0443\u0437\u0435\u044f \u041c\u044b\u0448\u0438, \u043c\u0443\u0437\u0435\u044f \u0421\u0442\u043e\u043b\u0438\u0446\u044b \u041b\u043e\u0446\u043c\u0430\u043d\u043e\u0432 (\u0438\u043b\u0438 \u043c\u0443\u0437\u0435\u0439 \u0432\u043e\u0434\u043e\u0447\u043d\u0438\u043a\u0430 \u041f.\u0410. \u0421\u043c\u0438\u0440\u043d\u043e\u0432\u0430) \u0438 \u043c\u0443\u0437\u0435\u044f \u0434\u0435\u0440\u0435\u0432\u044f\u043d\u043d\u043e\u0433\u043e \u0437\u043e\u0434\u0447\u0435\u0441\u0442\u0432\u0430 \u0438 \u0441\u0442\u0430\u0440\u0438\u043d\u043d\u043e\u0439 \u0442\u0435\u0445\u043d\u0438\u043a\u0438 \u043f\u043e\u0434 \u043e\u0442\u043a\u0440\u044b\u0442\u044b\u043c \u043d\u0435\u0431\u043e\u043c. (\u0433\u0440\u0443\u043f\u043f\u0430 \u043e\u0442 20 \u0447\u0435\u043b.)&nbsp; \u041f\u0440\u043e\u0434\u043e\u043b\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u044b 1,5 \u0447\u0430\u0441\u0430.\r\n\u0437\u0430 \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u0443\u044e \u043f\u043b\u0430\u0442\u0443\r\n\t\u0424\u043e\u043b\u044c\u043a\u043b\u043e\u0440\u043d\u0430\u044f \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0430 &quot;\u0412 \u0433\u043e\u0441\u0442\u0438 \u043a \u043c\u0435\u043b\u044c\u043d\u0438\u043a\u0443 \u043d\u0430 \u0431\u043b\u0438\u043d\u044b&quot;. (\u0433\u0440\u0443\u043f\u043f\u0430 \u043e\u0442 5 \u0447\u0435\u043b.)&nbsp;\u0425\u043e\u0437\u044f\u0439\u043a\u0430-\u043c\u0435\u043b\u044c\u043d\u0438\u0447\u0438\u0445\u0430, \u043e\u0436\u0438\u0434\u0430\u044f \u0433\u043e\u0441\u0442\u0435\u0439, \u0437\u0430\u0442\u043e\u043f\u0438\u0442 \u043f\u0435\u0447\u044c, \u0438\u0441\u043f\u0435\u0447\u0435\u0442 \u0431\u043b\u0438\u043d\u043e\u0432, \u0441 \u0448\u0443\u0442\u043a\u043e\u0439-\u043f\u0440\u0438\u0431\u0430\u0443\u0442\u043a\u043e\u0439 \u0440\u0430\u0441\u0441\u043a\u0430\u0436\u0435\u0442 \u043e \u0436\u0438\u0442\u044c\u0435-\u0431\u044b\u0442\u044c\u0435 \u043c\u0435\u043b\u044c\u043d\u0438\u043a\u0430 \u0438 \u0435\u0433\u043e \u0441\u0435\u043c\u044c\u0438, \u043f\u043e\u043a\u0430\u0436\u0435\u0442 \u0434\u043e\u043c \u0438 \u043f\u0440\u0438\u0433\u043b\u0430\u0441\u0438\u0442 \u0437\u0430 \u0441\u0442\u043e\u043b.\r\n\u0437\u0430 \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u0443\u044e \u043f\u043b\u0430\u0442\u0443\r\n\t\r\n\t\t&nbsp;&laquo;\u041c\u044b\u0448\u043a\u0438\u043d \u043f\u0440\u044f\u043d\u0438\u043a&raquo;. \u041c\u0430\u0441\u0442\u0435\u0440-\u043a\u043b\u0430\u0441\u0441 \u043f\u043e \u0440\u043e\u0441\u043f\u0438\u0441\u0438 \u043f\u0440\u044f\u043d\u0438\u043a\u0430 \u0446\u0432\u0435\u0442\u043d\u044b\u043c \u0448\u043e\u043a\u043e\u043b\u0430\u0434\u043e\u043c. \u042d\u0442\u043e \u0443\u0432\u043b\u0435\u043a\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0435, \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e\u0435 \u0437\u0430\u043d\u044f\u0442\u0438\u0435 \u043a\u0430\u043a \u0434\u043b\u044f \u0434\u0435\u0442\u0435\u0439, \u0442\u0430\u043a \u0438 \u0434\u043b\u044f \u0432\u0437\u0440\u043e\u0441\u043b\u044b\u0445! \u041a\u0430\u0436\u0434\u044b\u0439 \u0443\u0447\u0430\u0441\u0442\u043d\u0438\u043a \u043f\u043e\u043b\u0443\u0447\u0430\u0435\u0442 \u043f\u043e 2 \u043f\u0440\u044f\u043d\u0438\u043a\u0430 (\u043a\u043e\u0448\u043a\u0430 \u0438 \u043c\u044b\u0448\u043a\u0430) \u0438 \u043a\u043e\u0440\u043e\u0431\u043e\u0447\u043a\u0443 \u0441 \u0446\u0432\u0435\u0442\u043d\u044b\u043c \u0448\u043e\u043a\u043e\u043b\u0430\u0434\u043e\u043c. (\u0433\u0440\u0443\u043f\u043f\u0430 \u043e\u0442 5 \u0447\u0435\u043b.) \u041f\u0440\u043e\u0434\u043e\u043b\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u043c\u0430\u0441\u0442\u0435\u0440-\u043a\u043b\u0430\u0441\u0441\u0430 45-50 \u043c\u0438\u043d\u0443\u0442.","note":"","additional_excursions":[],"description_en":""},"538509":{"city":"\u041c\u043e\u0441\u043a\u0432\u0430","date_start":"09.08.2020","time_start":"22:00","date_end":"09.08.2020","time_end":"22:00","description":"","note":"","additional_excursions":[],"description_en":""}}