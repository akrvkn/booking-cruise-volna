u0445\u043e\u0434\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f &laquo;\u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u043e\u0432&raquo;\n\u0421\u0442\u0430\u0440\u0438\u043d\u043d\u044b\u0439 \u0433\u043e\u0440\u043e\u0434 \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u043e\u0432 \u0437\u0430\u0432\u043e\u0440\u0430\u0436\u0438\u0432\u0430\u0435\u0442 \u0438 \u043f\u0440\u0438\u0432\u043e\u0434\u0438\u0442 \u0432 \u043c\u0438\u0441\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0439 \u0442\u0440\u0435\u043f\u0435\u0442 \u043f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u043e \u0441\u043e\u0445\u0440\u0430\u043d\u0438\u0432\u0448\u0438\u043c\u0438\u0441\u044f \u0434\u0440\u0435\u0432\u043d\u0438\u043c\u0438 \u0441\u0442\u0440\u043e\u0435\u043d\u0438\u044f\u043c\u0438, \u0433\u0434\u0435 \u043f\u043e\u0434\u0447\u0430\u0441 \u0442\u0432\u043e\u0440\u0438\u043b\u0438\u0441\u044c \u0432\u0435\u043b\u0438\u043a\u0438\u0435, \u0430 \u0438\u043d\u043e\u0433\u0434\u0430 \u0438 \u0441\u0442\u0440\u0430\u0448\u043d\u044b\u0435 \u0432\u0435\u0449\u0438. \u0412\u0435\u0434\u044c \u0438\u043c\u0435\u043d\u043d\u043e \u0437\u0434\u0435\u0441\u044c \u0431\u044b\u043b\u0430 \u0440\u0435\u0437\u0438\u0434\u0435\u043d\u0446\u0438\u044f \u0418\u0432\u0430\u043d\u0430 \u0413\u0440\u043e\u0437\u043d\u043e\u0433\u043e, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043d\u0430 \u043f\u0440\u043e\u0442\u044f\u0436\u0435\u043d\u0438\u0438 17 \u043b\u0435\u0442 \u0432\u0435\u0440\u0448\u0438\u043b \u043e\u0442\u0441\u044e\u0434\u0430 \u0441\u0432\u043e\u0439 \u0441\u0443\u0434 \u0438 \u0443\u043f\u0440\u0430\u0432\u043b\u044f\u043b \u0441\u0443\u0434\u044c\u0431\u0430\u043c\u0438 \u043b\u044e\u0434\u0435\u0439. \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u043e\u0432\u0441\u043a\u0430\u044f \u0441\u043b\u043e\u0431\u043e\u0434\u0430&nbsp;- \u0440\u0435\u0434\u0447\u0430\u0439\u0448\u0438\u0439 \u0434\u0432\u043e\u0440\u0446\u043e\u0432\u043e-\u0445\u0440\u0430\u043c\u043e\u0432\u044b\u0439 \u043a\u043e\u043c\u043f\u043b\u0435\u043a\u0441&nbsp;\u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u043e\u0432\u0441\u043a\u043e\u0433\u043e \u041a\u0440\u0435\u043c\u043b\u044f, \u043f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u043e \u0441\u043e\u0445\u0440\u0430\u043d\u0438\u0432\u0448\u0438\u0439\u0441\u044f \u0434\u043e \u043d\u0430\u0448\u0438\u0445 \u0434\u043d\u0435\u0439 \u0438 \u043e\u0442\u043b\u0438\u0447\u043d\u043e \u043e\u0442\u0440\u0435\u0441\u0442\u0430\u0432\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0439. \u041d\u0435 \u043c\u0435\u043d\u0435\u0435 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u044b&nbsp;\u0445\u0440\u0430\u043c\u044b \u0410\u043b\u0435\u043a\u0441\u0430\u043d\u0434\u0440\u043e\u0432\u0441\u043a\u043e\u0439 \u0441\u043b\u043e\u0431\u043e\u0434\u044b. \u0412 \u0422\u0440\u043e\u0438\u0446\u043a\u043e\u0439, \u043d\u044b\u043d\u0435 \u041f\u043e\u043a\u0440\u043e\u0432\u0441\u043a\u043e\u0439 \u0446\u0435\u0440\u043a\u0432\u0438 \u0441\u043e\u0445\u0440\u0430\u043d\u0438\u043b\u0438\u0441\u044c&nbsp;\u043f\u043e\u0434\u043b\u0438\u043d\u043d\u044b\u0435 \u0444\u0440\u0435\u0441\u043a\u0438 1570-\u0445 \u0433\u043e\u0434\u043e\u0432.&nbsp; \u0421\u0432\u044f\u0442\u043e\u0439 \u0421\u0442\u0435\u0444\u0430\u043d \u041c\u0430\u0445\u0440\u0438\u0449\u0441\u043a\u0438\u0439&nbsp;&ndash; \u043e\u0434\u0438\u043d \u0438\u0437 \u0432\u0435\u043b\u0438\u043a\u0438\u0445 \u0441\u0432\u044f\u0442\u044b\u0445 \u0420\u0443\u0441\u0441\u043a\u043e\u0439 \u041f\u0440\u0430\u0432\u043e\u0441\u043b\u0430\u0432\u043d\u043e\u0439 \u0446\u0435\u0440\u043a\u0432\u0438. \u0412\u0435\u0434\u043e\u043c\u044b\u0439 \u0423\u043a\u0430\u0437\u0430\u043d\u0438\u0435\u043c \u0441\u0432\u044b\u0448\u0435,&nbsp;\u0432 \u043d\u0435\u043f\u0440\u043e\u0445\u043e\u0434\u0438\u043c\u044b\u0445 \u0434\u0435\u0431\u0440\u044f\u0445 \u0412\u043b\u0430\u0434\u0438\u043c\u0438\u0440\u0441\u043a\u0438\u0445 \u043b\u0435\u0441\u043e\u0432&nbsp;\u043d\u0430 \u043c\u0435\u0441\u0442\u0435 \u0441\u043b\u0438\u044f\u043d\u0438\u044f \u0440\u0435\u043a \u041c\u043e\u043b\u043e\u043a\u0447\u0438 \u0438 \u041c\u0430\u0445\u0440\u0438\u0449\u0435, \u043e\u043d \u043e\u0441\u043d\u043e\u0432\u044b\u0432\u0430\u0435\u0442 \u0432 1353 \u0433\u043e\u0434\u0443&nbsp;\u0421\u0432\u044f\u0442\u043e-\u0422\u0440\u043e\u0438\u0446\u043a\u0438\u0439 \u0421\u0442\u0435\u0444\u0430\u043d\u043e-\u041c\u0430\u0445\u0440\u0438\u0449\u0441\u043a\u0438\u0439 \u043c\u043e\u043d\u0430\u0441\u0442\u044b\u0440\u044c. \u041d\u043e\u0432\u0443\u044e \u043e\u0431\u0438\u0442\u0435\u043b\u044c \u043d\u0435\u043e\u0434\u043d\u043e\u043a\u0440\u0430\u0442\u043d\u043e \u043f\u043e\u0441\u0435\u0449\u0430\u043b \u0441\u0432\u044f\u0442\u043e\u0439 \u043f\u0440\u0435\u043f\u043e\u0434\u043e\u0431\u043d\u044b\u0439&nbsp;\u0421\u0435\u0440\u0433\u0438\u0439 \u0420\u0430\u0434\u043e\u043d\u0435\u0436\u0441\u043a\u0438\u0439, \u043a\u043e\u0442\u043e\u0440\u043e\u0433\u043e \u0441\u043e \u0441\u0432\u044f\u0442\u044b\u043c \u0421\u0442\u0435\u0444\u0430\u043d\u043e\u043c \u041c\u0430\u0445\u0440\u0438\u0449\u0441\u043a\u0438\u043c \u0441\u0432\u044f\u0437\u044b\u0432\u0430\u043b\u0430 \u0441\u0430\u043c\u0430\u044f \u0441\u0435\u0440\u0434\u0435\u0447\u043d\u0430\u044f \u0434\u0440\u0443\u0436\u0431\u0430. \u0421\u0442\u0435\u0444\u0430\u043d\u043e-\u041c\u0430\u0445\u0440\u0438\u0449\u0441\u043a\u0438\u0439 \u0436\u0435\u043d\u0441\u043a\u0438\u0439 \u043c\u043e\u043d\u0430\u0441\u0442\u044b\u0440\u044c - \u043e\u0434\u0438\u043d \u0438\u0437 \u0441\u0430\u043c\u044b\u0445 \u0431\u043b\u0430\u0433\u043e\u0443\u0441\u0442\u0440\u043e\u0435\u043d\u043d\u044b\u0445 \u0432 \u0420\u043e\u0441\u0441\u0438\u0438. \u041d\u0430 \u0435\u0433\u043e \u0442\u0435\u0440\u0440\u0438\u0442\u043e\u0440\u0438\u0438 \u0432\u043e\u0437\u0432\u0435\u0434\u0435\u043d \u0434\u0430\u0436\u0435 \u043f\u0430\u0442\u0440\u0438\u0430\u0440\u0448\u0438\u0439 \u0434\u0432\u043e\u0440\u0435\u0446, \u0441\u0440\u0443\u0431\u043b\u0435\u043d\u043d\u044b\u0439 \u0438\u0437 \u043e\u0433\u0440\u043e\u043c\u043d\u044b\u0445 \u0431\u0440\u0435\u0432\u0435\u043d! \u0422\u0435\u0440\u0440\u0438\u0442\u043e\u0440\u0438\u044f \u043c\u043e\u043d\u0430\u0441\u0442\u044b\u0440\u044f \u043d\u0435\u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e \u0443\u044e\u0442\u043d\u0430\u044f, \u0443\u0445\u043e\u0436\u0435\u043d\u043d\u0430\u044f, \u0443\u043a\u0440\u0430\u0448\u0435\u043d\u043d\u0430\u044f \u043c\u043d\u043e\u0433\u043e\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u044b\u043c\u0438 \u0446\u0432\u0435\u0442\u043d\u0438\u043a\u0430\u043c\u0438, \u043c\u043e\u0436\u0436\u0435\u0432\u0435\u043b\u044c\u043d\u0438\u043a\u043e\u043c \u0438 \u043a\u0435\u0434\u0440\u043e\u0432\u044b\u043c \u0441\u0442\u043b\u0430\u043d\u0438\u043a\u043e\u043c, \u0434\u0430\u0436\u0435 \u0441 \u043f\u0440\u0443\u0434\u043e\u043c, \u0433\u0434\u0435 \u0432\u043e\u0434\u044f\u0442\u0441\u044f \u0443\u0442\u043a\u0438 \u044d\u043a\u0437\u043e\u0442\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u043f\u043e\u0440\u043e\u0434.&nbsp;\n3. \u0410\u0432\u0442\u043e-\u043f\u0435\u0448\u0435\u0445\u043e\u0434\u043d\u0430\u044f \u044d\u043a\u0441\u043a\u0443\u0440\u0441\u0438\u044f &laquo;\u041a\u0430\u0448\u0438\u043d&raquo;\n\u0413\u043e\u0440\u043e\u0434 \u041a\u0430\u0448\u0438\u043d - \u043e\u0434\u0438\u043d \u0438\u0437 \u0441\u0442\u0430\u0440\u0435\u0439\u0448\u0438\u0445 \u0432 \u0422\u0432\u0435\u0440\u0441\u043a\u043e\u0439 \u043e\u0431\u043b\u0430\u0441\u0442\u0438, \u0440\u0430\u0441\u043f\u043e\u043b\u043e\u0436\u0435\u043d\u043d\u044b\u0439 \u043d\u0430 \u0436\u0438\u0432\u043e\u043f\u0438\u0441\u043d\u044b\u0445 \u0431\u0435\u0440\u0435\u0433\u0430\u0445 \u0440\u0435\u043a\u0438 \u041a\u0430\u0448\u0438\u043d\u043a\u0438. \u0420\u0435\u043a\u0430, \u043f\u0440\u043e\u0442\u0435\u043a\u0430\u044e\u0449\u0430\u044f \u043f\u043e \u0433\u043e\u0440\u043e\u0434\u0443, \u0444\u043e\u0440\u043c\u0438\u0440\u0443\u0435\u0442 \u0442\u043e\u0447\u043d\u044b\u0439 \u0441\u0438\u043b\u0443\u044d\u0442 \u0441\u0435\u0440\u0434\u0446\u0430, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u041a\u0430\u0448\u0438\u043d \u043d\u0430\u0437\u044b\u0432\u0430\u044e\u0442 &laquo;\u0433\u043e\u0440\u043e\u0434\u043e\u043c \u0440\u043e\u0441\u0441\u0438\u0439\u0441\u043a\u043e\u0433\u043e \u0441\u0435\u0440\u0434\u0446\u0430&raquo;.\n\u041a\u0430\u043b\u044f\u0437\u0438\u043d - \u0433\u043e\u0440\u043e\u0434 \u043d\u0430 2\/3 \u0437\u0430\u0442\u043e\u043f\u043b\u0435\u043d \u0432\u043e\u0434\u0430\u043c\u0438 \u0423\u0433\u043b\u0438\u0447\u0441\u043a\u043e\u0433\u043e \u0432\u043e\u0434\u043e\u0445\u0440\u0430\u043d\u0438\u043b\u0438\u0449\u0430. \u041e\u043a\u0430\u0437\u0430\u0432\u0448\u0438\u0441\u044c \u043d\u0430 \u0431\u0435\u0440\u0435\u0433\u0443 \u0412\u043e\u043b\u0433\u0438, \u043f\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0438\u043a \u0441 \u0443\u0434\u0438\u0432\u043b\u0435\u043d\u0438\u0435\u043c \u043e\u0431\u043d\u0430\u0440\u0443\u0436\u0438\u0432\u0430\u0435\u0442 \u0437\u0430\u0442\u043e\u043f\u043b\u0435\u043d\u043d\u0443\u044e \u043a\u043e\u043b\u043e\u043a\u043e\u043b\u044c\u043d\u044e, \u0441\u0442\u043e\u044f\u0449\u0443\u044e \u043f\u0440\u044f\u043c\u043e \u043f\u043e\u0441\u0435\u0440\u0435\u0434\u0438\u043d\u0435 \u0432\u043e\u0434\u043e\u0445\u0440\u0430\u043d\u0438\u043b\u0438\u0449\u0430. \u0412\u0435\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u0432\u043e\u0437\u0432\u044b\u0448\u0430\u044e\u0449\u0430\u044f\u0441\u044f \u0441\u0440\u0435\u0434\u0438 \u0432\u043e\u0434\u043d\u044b\u0445 \u043f\u0440\u043e\u0441\u0442\u043e\u0440\u043e\u0432, \u043e\u043d\u0430 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043d\u0435\u043f\u0440\u0435\u0441\u0442\u0430\u043d\u043d\u044b\u043c \u0438 \u0431\u0435\u0437\u043c\u043e\u043b\u0432\u043d\u044b\u043c \u043d\u0430\u043f\u043e\u043c\u0438\u043d\u0430\u043d\u0438\u0435\u043c \u043e \u043c\u043d\u043e\u0433\u043e\u0432\u0435\u043a\u043e\u0432\u043e\u0439 \u0438 \u0441\u043b\u0430\u0432\u043d\u043e\u0439 \u0438\u0441\u0442\u043e\u0440\u0438\u0438 \u041a\u0430\u043b\u044f\u0437\u0438\u043d\u0441\u043a\u043e\u0439 \u0437\u0435\u043c\u043b\u0438. \u0417\u0434\u0435\u0441\u044c \u0412\u044b \u0443\u0437\u043d\u0430\u0435\u0442\u0435 \u043e \u0442\u043e\u043c, \u0447\u0442\u043e \u043e\u0437\u043d\u0430\u0447\u0430\u0435\u0442 \u043d\u0430\u0437\u0432\u0430\u043d\u0438\u0435 &laquo;\u041a\u0430\u043b\u044f\u0437\u0438\u043d&raquo;, \u043a\u0430\u043a \u0432\u043e\u0437\u043d\u0438\u043a \u0433\u043e\u0440\u043e\u0434 \u0438 \u0447\u0435\u043c \u0436\u0438\u043b \u043d\u0430 \u043f\u0440\u043e\u0442\u044f\u0436\u0435\u043d\u0438\u0438 13 \u0441\u0442\u043e\u043b\u0435\u0442\u0438\u0439, \u0430 \u0442\u0430\u043a\u0436\u0435, \u0447\u0442\u043e \u0442\u0430\u043a\u043e\u0435 \u043a\u043e\u043a\u043b\u044e\u0448\u043a\u0438 \u0438 \u043a\u0430\u043a \u0438\u0437\u0433\u043e\u0442\u0430\u0432\u043b\u0438\u0432\u0430\u043b\u0438 \u0432 \u0441\u0442\u0430\u0440\u0438\u043d\u0443 \u0438\u0437\u0440\u0430\u0437\u0446\u044b.\n&nbsp;","note":"","additional_excursions":[],"description_en":""},"517277":{"city":"\u041c\u043e\u0441\u043a\u0432\u0430","date_start":"22.05.2020","time_start":"20:00","date_end":"22.05.2020","time_end":"20:00","description":"","note":"","additional_excursions":[],"description_en":""}}